@{
    ViewData["Title"] = "UploadFile";
    Layout = "/Views/Shared/_Layout.cshtml";
}

<h2>UploadFile</h2>
<form method="post" enctype="multipart/form-data" asp-controller="Upload" asp-action="UploadFile">
    <br>
    <div class="form-group">
        <div class="col-md-10" style="border:solid;border-inline-width:thin;">
            @*<p>Upload one or more files using this form:</p>*@
            <input type="file" name="files" accept=" .pbix" id="myFile" onchange=" getFileDate()" required/>
        </div>
    </div>
    <br>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Upload"/>
        </div>
    </div>
</form>